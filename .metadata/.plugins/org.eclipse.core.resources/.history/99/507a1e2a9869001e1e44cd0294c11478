package com.SmallEx;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String str="20;30;40;50";
		//str.split(";");
		String[] strArray = str.split(";");
		
		
		for (int i = 0; i < strArray.length; i++) {
			System.out.println(strArray[i].valueOf(i));
			 
		}
		 

	}

}
