package com.cg2;

public class Test {

	public static void main(String[] args) {
		ArrayDemo arraydemo = new ArrayDemo();
		int[] ar = {2,3,4,5};
		arraydemo.printArray(ar);
		
		char[] ch = {'a', 'b', 'c', 'd'};
		arraydemo.printArray(ch);
		
		double[] dl = {1.2, 2.4, 5.6};
		arraydemo.printArray(dl);
		
		String[] st = {"sujal", "manikanta", "Bharat"};
		arraydemo.printArray(st);
		
		
		
		
		Course python = new Course(121,"Python",4000);
		Course java = new Course(122,"Java",7000);
		Course angular = new Course(123,"Angular",7000);
		
		Course[] course = {python, java, angular};
		
		Student student = new Student(1,"harsh",course);
		
		
		System.out.println(student);
		
		

	}

}
