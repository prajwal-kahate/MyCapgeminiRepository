package com.cg2;

public class ArrayDemo {
	// public void printArray(int []);
		// public void printArray(char []);
		// public void printArray(double []);
		// public void printArray(String []);
	
	public void printArray(int[] arr)
	{
		for (int a:arr) {
			System.out.print(a+" ");
		}
		System.out.println();
	}
	public void printArray(char[] arr)
	{
		for (char a:arr) {
			System.out.print(a+" ");
		}
		System.out.println();
	}
	
	
	public void printArray(double[] arr)
	{
		for (double a:arr) {
			System.out.print(a+" ");
		}
		System.out.println();
	}
	
	// public void printArray(String []);
	public void printArray(String[] arr)
	{
		for (String a:arr) {
			System.out.print(a+" ,");
		}
		System.out.println();
	}

}
